{
  "manifest_version": 2,
  "name": "Pages 404 Link",
  "version": "1.0",

  "description": "Provides a link to the GitHub Repo and Owner's Profile for a 404 GitHub Page",

  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.github.io/*"],
      "js": ["pages404link.js"]
    }
  ]
}
